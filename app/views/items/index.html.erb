<p id="notice"><%= notice %></p>

<h1>Listing Items</h1>

<div class="container">
  <div class="center">
    <div class="row">
      <% @items.each do |item| %>
        <div class="col-md-3">
          <div class="thumbnail">
            <div class="caption">
              <h3><%= item.name %></h3>
              <p><%= number_to_currency(item.price) %></p>
              <p><%= "Sold by " %></p>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>

  <br>
</div>


<%= link_to 'New Item', new_item_path %>
